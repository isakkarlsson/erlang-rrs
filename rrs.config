{'evaluation.methods',
 [
  {<<"cross-validation">>, [{name, <<"Cross-Validation">>},
			    {help, <<"Perform cross-validation">>},
			    {options, [{folds, [{name, <<"Folds">>},
						{type, <<"numeric">>},
						{min, 2},
						{max, <<"no_examples">>},
						{step, 1},
						{default, 10},
						{help, <<"Number of cross-validation folds">>}]},
				       {store, [{name, <<"Store Model">>},
						{type, <<"categoric">>},
						{options, [{yes, [{name, <<"Yes">>},
								  {help, <<"">>}]},
							   {no, [{name, <<"No">>},
								 {help, <<"">>}]}]}]}
				      ]}]},
  {<<"split-validation">>, [{name, <<"Split-Validation">>},
			    {help, <<"Split data set into two parts - training and testing">>},
			    {options, [{ratio, [{name, <<"Training set size">>},
						{type, <<"numeric">>},
						{min, 10},
						{max, 100},
						{step, 1},
						{default, 66},
						{help, <<"Size of the training set (as a precentage)">>}]}]}]}
 ]
}.
   
