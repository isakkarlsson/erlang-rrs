{'log.target', std_err}.
{'log.level', debug}.
{'port', 8080}.
{'ui.path', "ui"}.
{'models.path', "models"}.
{'models.ext', ".rr"}.

{'machine-learning.methods',
 [
  {<<"rf">>, [
	      {name, <<"Random Forest">>},
	      {help, <<"No help">>},
	      {'options', [{'no_features', [{name, <<"Number of features">>},
					    {'type', <<"numeric">>},
					    {'min', 1},
					    {step, 1},
					    {'max', <<"no_features">>}, %% no_features, no_examples
					    {default, <<"log">>}, %% log or number or undefined
					    {'help', <<"Informative help text">>}]},
			   {'no_trees',    [{name, <<"Number of trees">>},
					    {'type', <<"numeric">>},
					    {'min', 0},
					    {step, 100},
					    {'max', 10000},
					    {default, 1000},
					    {'help', <<"">>}]},
			   {'max_depth', [{type, <<"numeric">>},
					   {name, <<"Maximal depth of individual tree">>},
					   {min, 1},
					   {max, 1000},
					   {step, 1},
					   {default, 500},
					   {help, <<"The maximum depth of individual trees can increse diversity and lower computational cost (if minimized)">>}]},
			   {'min_examples', [{type, <<"numeric">>},
					     {name, <<"Minimum examples to allow splitting">>},
					     {min, 1},
					     {max, <<"no_examples">>},
					     {default, 2},
					     {step, 1},
					     {help, <<"The number of examples that are allowed for splitting a node controls....">>}]},
			   {'distribute', [{type, <<"categoric">>},
					   {name, <<"Distribute examples">>},
					   {options, [{default, [{name, <<"Default">>},
								 {help, <<"">>}]},
						      {rule,    [{name, <<"Rule">>},
								 {help, <<"">>}]}]}]},					      
			   {'missing', [{type, <<"categoric">>},
					{name, <<"Distribute missing values">>},
					{options, [{'random',  [{name, <<"Random distribution">>},
								{help, <<"">>}
							       ]},
						   {'weighted', [{name, <<"Weighted distribution">>},
								 {help, <<"">>}
								]},
						   {'randomw', [{name, <<"Random Weighted Distribution">>},
								{help, <<"">>}]},
						   {partition, [{name, <<"Randomly Partition Fractions">>},
								{help, <<"">>}]},
						   {wpartition, [{name, <<"Weighted Partition of Fractions">>},
								 {help, <<"">>}]}
						  ]}]},
			   {'feature_sampling', [{type, <<"categoric">>},
						 {name, <<"Feature Sampling">>},
						 {options, [{subset, [{name, <<"Default Subset">>},
								      {help, <<"">>}]},
							    {weka, [{name, <<"Infinite Resample">>},
								    {help, <<"">>}]},
							    {rule, [{name, <<"Rule Nodes">>},
								    {help, <<"">>}]},
							    {'random-rule', [{name, <<"Random Choice Between 'Rule Nodes' and ' Default Subset'">>},
									     {help, <<"">>}]}
							   ]
						 }]},
			   {'no_rules', [{type, <<"categoric">>},
					 {name, <<"Number or Rules">>},
					 {options, [{ss, [{name, <<"Same/Same">>},
							  {help, <<"">>}]}]}]},
			   {'rule_score', [{type, <<"categoric">>},
					   {name, <<"Rule Optimize Measure">>},
					   {options, [{laplace, [{name, <<"Laplace Estimate">>},
								 {help, <<"">>}]},
						      {m, [{name, <<"M-Estimate">>},
							   {help, <<"">>}]},
						      {purity, [{name, <<"Purity Estimate">>},
								{help, <<"">>}]}]}]},					    
			   {score, [{type, <<"categoric">>},
				    {name, <<"Split measure">>},
				    {options, [{'info', [{name, <<"Information Entropy">>},
							 {help, <<"">>}]},
					       {'gini', [{name, <<"Gini Impurity">>},
							 {help, <<"">>}]},
					       {'gini-info', [{name, <<"Random Choice Between Entropy and Gini">>},
							      {help, <<"">>}]},
					       {'jensen-difference', [{name, <<"Jensen Difference">>},
								      {help, <<"">>}]}
					       
					      ]}

				   ]}							
			   
			  ]}
	     ]},
  {<<"rfr">>, [
	       {id, <<"rfr">>},
	       {name, <<"Random Rule Forest">>},
	       {help, <<"No help">>},
	       {options, []}
	      ]}
 ]
}.

